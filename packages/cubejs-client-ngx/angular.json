{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cubejs-client-ngx": {
      "root": ".",
      "sourceRoot": "src",
      "projectType": "library",
      "prefix": "ch",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "tsconfig.json",
            "project": "ng-package.json"
          }
        }
      }
    }
  },
  "defaultProject": "cubejs-client-ngx"
}